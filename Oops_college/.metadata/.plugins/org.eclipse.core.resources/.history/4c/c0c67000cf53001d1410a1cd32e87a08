import java.util.ArrayList;
import java.util.Collections;


public class ArrayListTest {

 public static void main(String[] args) {
  // TODO Auto-generated method stub

  ArrayList <Integer> nums=new ArrayList<Integer>();
  nums.add(20);
  nums.add(12);
  nums.add(23);
  nums.add(12);
  nums.add(5);
  
  for (int i=0;i<nums.size();i++)
   System.out.println(nums.get(i));
  
  nums.sort(null);
  System.out.println("sorted baba");
  for (int i=0;i<nums.size();i++)
   System.out.println(nums.get(i));
  Collections.reverse(nums);
  System.out.println("reverse baba");

  for (int i=0;i<nums.size();i++)
   System.out.println(nums.get(i));
  
  System.out.println("names baba");

  ArrayList <String> names=new ArrayList<String>();
  names.add("Vijay");
  names.add("Sanjana");
  names.add("Niranjan");
  for (int i=0;i<names.size();i++)
   System.out.println(names.get(i));
  
  ArrayList<Employee> emps= new ArrayList<Employee>();
  emps.add(new Employee("Harsh",20));
  emps.add   (new Employee("Lokesh",55));
   emps.add(new Employee("Leban",18));
   emps.add(new Employee("Akansha",100));

  for(Employee e :emps)
   System.out.println(e);
   
  emps.sort(null);
  
  System.out.println("sorted emps baba");

  for(Employee e :emps)
   System.out.println(e);
 }
 
 
 

}

class Employee implements Comparable<Employee>
{
 private String ename;
 private int age;
 public Employee(String name,int age)
 {
  this.ename=name;
  this.age=age;
 }
 
 public String toString()
 {
  return(ename +":"+ age);
 }

 @Override
 public int compareTo(Employee arg0) {
  // TODO Auto-generated method stub
  return  (this.age-arg0.age);
 }
 
 
}
